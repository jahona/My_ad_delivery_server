<VAST version="4.2" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <Ad id="ad-id" adType="video">
    <InLine>
      <AdSystem version="4.0"></AdSystem>
      <AdTitle>jahona video ad example</AdTitle>
      
      <Impression id="imp-id"><![CDATA[http://localhost:8000/track/impression]]></Impression>
      <Description>example</Description>
      
      <Pricing model="CPM" currency="USD">
        <![CDATA[ 25.00 ]]>
      </Pricing>
      
      <Error>
        <![CDATA[http://localhost:8000/img/error.JPG]]>  
      </Error>

      <Creatives>
        <Creative id="creative-id" adId="ad-id" sequence="1">
          <Linear>
            <Duration>00:00:30</Duration>
            <MediaFiles>
              <MediaFile id="mediafile-id" delivery="progressive" type="video/mp4" bitrate="500" width="400" height="300" minBitrate="360" maxBitrate="1080" scalable="1" maintainAspectRatio="1" codec="0">
                                <![CDATA[http://localhost:8000/video/sample.mp4]]>
              </MediaFile>
            </MediaFiles>
          </Linear>
        </Creative>
      </Creatives>
    </InLine>
  </Ad>  
  <Error>
    <![CDATA[http://localhost:8000/img/error.JPG]]>
  </Error>
</VAST>